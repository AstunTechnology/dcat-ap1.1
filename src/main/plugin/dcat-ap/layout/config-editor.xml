		<for name="vcard:Address" templateModeOnly="true">	
			<template>
				<values>
					<key label="FieldId121" xpath="@rdf:about"/>	
					<key label="FieldId122" required="true" xpath="vcard:street-address" or="street-address" in="/rdf:RDF/dcat:Catalog/dcat:dataset/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address"/>	
					<key label="FieldId123" required="true" xpath="vcard:locality" or="locality" in="/rdf:RDF/dcat:Catalog/dcat:dataset/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address"/>	
					<key label="FieldId124" required="true" xpath="vcard:postal-code" or="postal-code" in="/rdf:RDF/dcat:Catalog/dcat:dataset/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address"/>	
					<key label="FieldId125" required="true" xpath="vcard:country-name" or="country-name" in="/rdf:RDF/dcat:Catalog/dcat:dataset/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address"/>
				</values>
				<snippet>
					<vcard:Address rdf:about="{{FieldId121}}">
						<vcard:street-address>{{FieldId122}}</vcard:street-address>
						<vcard:locality>{{FieldId123}}</vcard:locality>
						<vcard:postal-code>{{FieldId124}}</vcard:postal-code>
						<vcard:country-name>{{FieldId125}}</vcard:country-name>
					</vcard:Address>
				</snippet>
			</template>
		</for>
		
