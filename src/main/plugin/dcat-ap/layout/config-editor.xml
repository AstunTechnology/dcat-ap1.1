<?xml version="1.0" encoding="UTF-8"?>
<editor xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:spdx="http://spdx.org/rdf/terms#" xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:adms="http://www.w3.org/ns/adms#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dct="http://purl.org/dc/terms/" xmlns:dcat="http://www.w3.org/ns/dcat#" xmlns:vcard="http://www.w3.org/2006/vcard/ns#" xmlns:foaf="http://xmlns.com/foaf/0.1/" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:schema="http://schema.org/" xsi:noNamespaceSchemaLocation="../../../../../../config-editor.xsd">
	<!-- Define form field types per metadata element. -->
	<fields>
		<for name="dct:title" use="textarea"/>
		<for name="dct:description" use="textarea"/>
		<for name="vcard:hasEmail" use="email"/>
<!-- 		<for name="dcat:mediaType" use="data-gn-keyword-picker" />-->
		<for name="dct:format" use="data-gn-keyword-picker" />
		<for name="dct:type" use="data-gn-keyword-picker" />
		<for name="dct:license" use="data-gn-keyword-picker"/>
		<for name="dct:language" use="data-gn-keyword-picker"/>
		<for name="dct:issued" use="data-gn-date-picker"/>
		<for name="dct:modified" use="data-gn-date-picker"/>
		<for name="schema:startDate" use="data-gn-date-picker"/>
		<for name="schema:endDate" use="data-gn-date-picker"/>
<!--  		<for name="dcat:keyword" use="data-gn-keyword-picker"/>-->
		<for name="dcat:theme" use="data-gn-keyword-picker" />
		
	</fields>
	<!-- Complex fields. -->
	<fieldsWithFieldset>
		<name>dcat:Dataset</name>
		<name>dcat:distribution</name>
		<name>dcat:Distribution</name>
		<name>spdx:checksum</name>
		<name>spdx:Checksum</name>
		<name>foaf:page</name>
		<name>foaf:Document</name>
<!--
		<name>dcat:mediaType</name>
-->
		<name>dct:format</name>
	</fieldsWithFieldset>
	<transparentFieldset>
<!--  
		<name>dcat:Distribution</name>
		<name>spdx:Checksum</name>
		<name>foaf:Document</name>
-->
	</transparentFieldset>
	<!-- View configuration -->
	<views>
    <view name="default">
	      <sidePanel>
	        <directive data-gn-validation-report=""/>
	      </sidePanel>
			<tab id="default" default="true">
				<!-- DON'T MOVE THIS SECTION, otherwise rendering stops at a certain point in the fields list -->
				<section>
					<section name="metadata">
						<field xpath="/dcat:Dataset/dct:title" or="title" in="/dcat:Dataset/*"/>
						<action type="add" if="count(/dcat:Dataset/dct:title)=0" or="title" in="/dcat:Dataset">
							<template>
								<snippet>
									<dct:title/>
								</snippet>
							</template>
						</action>
						<field xpath="/dcat:Dataset/dct:description" or="description" in="/dcat:Dataset/*" />
						<action type="add" if="count(/dcat:Dataset/dct:description)=0" or="description" in="/dcat:Dataset">
							<template>
								<snippet>
									<dct:description/>
								</snippet>
							</template>
						</action>
						<field xpath="/dcat:Dataset/dct:accessRights" or="accessRights" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:issued" or="issued" in="/dcat:Dataset/*" del="."/>
						<field xpath="/dcat:Dataset/dct:modified" or="modified" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/owl:versionInfo" or="versionInfo" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/adms:versionNotes" or="versionNotes" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dcat:theme" or="theme" in="/dcat:Dataset/*" />
						<action type="add" or="theme" in="/dcat:Dataset" if="count(/dcat:Dataset/dcat:theme)=0">
							<template>
								<snippet>
						            <dcat:theme>
						               <skos:Concept rdf:about="">
						                  <skos:prefLabel xml:lang="nl"/>
						                  <skos:prefLabel xml:lang="en"/>
						                  <skos:prefLabel xml:lang="fr"/>
						                  <skos:prefLabel xml:lang="de"/>
						                  <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/data-theme"/>
						               </skos:Concept>
						            </dcat:theme>
								</snippet>
							</template>
						</action>
 						<field xpath="/dcat:Dataset/dct:conformsTo" or="conformsTo" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:accrualPeriodicity" or="accrualPeriodicity" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:hasVersion" or="hasVersion" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:identifier" or="identifier" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:isVersionOf" or="isVersionOf" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dcat:landingPage">
							<template>
								<values>
									<key label="landingPage" xpath="@rdf:resource" tooltip="landingPage"/>
								</values>
								<snippet>
									<dcat:landingPage rdf:resource="{{landingPage}}"/>
								</snippet>
							</template>
						</field>
						<field xpath="/dcat:Dataset/dct:language" or="language" in="/dcat:Dataset/*" />
						<action type="add" or="language" in="/dcat:Dataset" if="count(/dcat:Dataset/dct:language)=0">
							<template>
								<snippet>
						            <dct:language>
						               <skos:Concept rdf:about="">
						                  <skos:prefLabel xml:lang="nl"/>
						                  <skos:prefLabel xml:lang="en"/>
						                  <skos:prefLabel xml:lang="fr"/>
						                  <skos:prefLabel xml:lang="de"/>
						                  <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/language"/>
						               </skos:Concept>
						            </dct:language>
								</snippet>
							</template>
						</action>
 						<field xpath="/dcat:Dataset/dct:provenance" or="provenance" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:relation" or="relation" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:source" or="source" in="/dcat:Dataset/*"/>
						<field xpath="/dcat:Dataset/dct:type" or="type" in="/dcat:Dataset/*"/>
						<action type="add" or="type" in="/dcat:Dataset" if="count(/dcat:Dataset/dct:type)=0">
							<template>
								<snippet>
						            <dct:type>
						               <skos:Concept rdf:about="">
						                  <skos:prefLabel xml:lang="nl"/>
						                  <skos:prefLabel xml:lang="en"/>
						                  <skos:prefLabel xml:lang="fr"/>
						                  <skos:prefLabel xml:lang="de"/>
						                  <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/resource-type"/>
						               </skos:Concept>
						            </dct:type>
								</snippet>
							</template>
						</action>
 					</section>
					<!-- Dataset - Contact point - Organization (Part 1) -->
					<section name="vcardOrganization">
						<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:fn" or="fn" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/*"/>
						<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:organization-name" or="organization-name" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/*"/>
						<!-- Dataset - Contact point - Organization - Address -->
						<section name="vcardAddress">
							<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/vcard:street-address" or="street-address" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/*"/>
							<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/vcard:locality" or="locality" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/*"/>
							<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/vcard:postal-code" or="postal-code" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/*"/>
							<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/vcard:country-name" or="country-name" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasAddress/vcard:Address/*"/>
						</section>
						<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasEmail" or="hasEmail" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/*"/>
						<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasURL" or="hasURL" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/*"/>
						<field xpath="/dcat:Dataset/dcat:contactPoint/vcard:Organization/vcard:hasTelephone" or="hasTelephone" in="/dcat:Dataset/dcat:contactPoint/vcard:Organization/*"/>
					</section>
					<section name="keywords">
						<field xpath="/dcat:Dataset/dcat:keyword" or="keyword" in="/dcat:Dataset/*"/>
					</section>
					<!-- Dataset - Publisher - Agent -->
					<section name="foafAgent">
						<field xpath="/dcat:Dataset/dct:publisher/foaf:Agent/foaf:name" or="name" in="/dcat:Dataset/dct:publisher/foaf:Agent/*"/>
						<field xpath="/dcat:Dataset/dct:publisher/foaf:Agent/dct:type" or="type" in="/dcat:Dataset/dct:publisher/foaf:Agent/*" />
						<action type="add" or="type" in="/dcat:Dataset/dct:publisher/foaf:Agent" if="count(/dcat:Dataset/dct:publisher/foaf:Agent/dct:type)=0">
							<template>
								<snippet>
						            <dct:type>
						               <skos:Concept rdf:about="">
						                  <skos:prefLabel xml:lang="nl"/>
						                  <skos:prefLabel xml:lang="en"/>
						                  <skos:prefLabel xml:lang="fr"/>
						                  <skos:prefLabel xml:lang="de"/>
										  <skos:inScheme rdf:resource="http://publications.europa.eu/resource/authority/organization-type"/>
						               </skos:Concept>
						            </dct:type>
								</snippet>
							</template>
						</action>
 					</section>
					<!-- Dataset - Identifier -->
					<section name="admsIdentifier">
						<field xpath="/dcat:Dataset/adms:identifier/adms:Identifier/skos:notation" or="notation" in="/dcat:Dataset/adms:identifier/adms:Identifier/*"/>
					</section>
					<!-- Dataset - Temporal -->
					<section name="dctPeriodOfTime">
						<field xpath="/dcat:Dataset/dct:temporal/dct:PeriodOfTime/schema:startDate" or="startDate" in="/dcat:Dataset/dct:temporal/dct:PeriodOfTime/*" del="."/>
						<field xpath="/dcat:Dataset/dct:temporal/dct:PeriodOfTime/schema:endDate" or="endDate" in="/dcat:Dataset/dct:temporal/dct:PeriodOfTime/*" del="."/>
					</section>
				</section>
			</tab>
